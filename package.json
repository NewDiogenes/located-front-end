{
  "name": "located-front-end",
  "version": "1.0.0",
  "description": "A webpage to allow users to sign up/request an invitiation for a yt to be completed service",
  "scripts": {
    "test": "jest --coverage",
    "test:dev": "npm run test -- --watchAll",
    "start": "webpack-dev-server",
    "lint": "eslint ./src ./webpack.config.js -f table --ext *.js --ext *.jsx",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "npm run test && npm run lint"
  },
  "author": "Located AKA Club Livid",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "styled-components": "^2.2.3",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/index.js"
    ],
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
